{
  "name": "discord-bot",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "type": "module",
  "scripts": {
    "start": "tsx src",
    "dev": "tsx watch src",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "migrate:latest": "tsx src/database/migrate/bin",
    "test": "vitest",
    "test:coverage": "vitest --coverage && open coverage/index.html",
    "gen:types": "kysely-codegen --camel-case --dialect sqlite --out-file src/database/types.ts && prettier --write src/database/types.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@tsconfig/node18": "^18.2.4",
    "@types/better-sqlite3": "^7.6.12",
    "@types/express": "^4.17.21",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.12.7",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "@vitest/coverage-v8": "^3.0.4",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "kysely": "^0.27.5",
    "kysely-codegen": "^0.17.0",
    "prettier": "^3.2.5",
    "supertest": "^7.0.0",
    "tsx": "^4.7.2",
    "vitest": "^3.0.2"
  },
  "dependencies": {
    "better-sqlite3": "^11.8.1",
    "discord.js": "^14.17.3",
    "dotenv": "^16.4.7",
    "express": "^4.19.2",
    "http-status-codes": "^2.3.0",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "zod": "^3.24.1"
  }
}
